  "name": "Infested",
  "description": "This mod adds infested block varients to things like mossy cobblestone and bookshelves, with world gen integration",
  "authors": [
    "Keckle"
  ],

  "license": "CC0-1.0",
  "icon": "assets/infested/icon.png",

  "environment": "*",
  "entrypoints": {
